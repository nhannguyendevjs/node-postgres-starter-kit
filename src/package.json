{
  "name": "node-postgres-starter-kit",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.mjs",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:coverage": "vitest run --coverage",
    "start": "npm run pure",
    "pure": "node --env-file=environments/.pure.env index.mjs",
    "dev": "node --env-file=environments/.dev.env index.mjs",
    "prod": "node --env-file=environments/.prod.env index.mjs",
    "docker": "npm run docker:build",
    "docker:build": "docker build -t node-postgres-starter-kit .",
    "docker:run": "docker run -d --name node-postgres-starter-kit -p 8080:8080 node-postgres-starter-kit",
    "docker:start": "docker start node-postgres-starter-kit",
    "docker:publish": "docker tag node-postgres-starter-kit hoainhaannguyen/node-postgres-starter-kit:latest && docker push hoainhaannguyen/node-postgres-starter-kit:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhannguyendevjs/node-postgres-starter-kit.git"
  },
  "keywords": [
    "node",
    "postgres",
    "starter",
    "kit"
  ],
  "author": "hoainhaanguyen@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhannguyendevjs/node-postgres-starter-kit/issues"
  },
  "homepage": "https://github.com/nhannguyendevjs/node-postgres-starter-kit#readme",
  "dependencies": {
    "@bull-board/express": "6.5.4",
    "@prisma/adapter-pg": "6.1.0",
    "@prisma/client": "6.1.0",
    "async": "3.2.6",
    "axios": "1.7.9",
    "bullmq": "5.34.6",
    "compression": "1.7.5",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "express": "4.21.2",
    "express-rate-limit": "7.5.0",
    "handlebars": "4.7.8",
    "helmet": "8.0.0",
    "ioredis": "5.4.2",
    "jsonwebtoken": "9.0.2",
    "luxon": "3.5.0",
    "mongodb": "6.12.0",
    "mongoose": "8.9.3",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.16",
    "numfmt": "3.1.4",
    "pg": "8.13.1",
    "redis": "4.7.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "ua-parser-js": "2.0.0",
    "winston": "3.17.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@vitest/coverage-istanbul": "2.1.8",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "pm2": "5.4.3",
    "prisma": "6.1.0",
    "vitest": "2.1.8"
  }
}
